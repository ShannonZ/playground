<Window x:Class="Jet64Heat.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
         xmlns:oxy="clr-namespace:OxyPlot.Wpf;assembly=OxyPlot.Wpf"
        xmlns:local="clr-namespace:Jet64Heat"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="850">
    <Grid x:Name="root">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="50*"/>
            <ColumnDefinition Width="600*"/>
            <ColumnDefinition Width="200*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="200*"/>
            <RowDefinition Height="600*"/>
        </Grid.RowDefinitions>
        <oxy:PlotView Model="{Binding model}"  Grid.Row="1" Grid.Column="1"/>
        <oxy:PlotView Model="{Binding xpmodel}"  Grid.Row="0" Grid.Column="1"/>
        <oxy:PlotView Model="{Binding ypmodel}"  Grid.Row="1" Grid.Column="2"/>
    </Grid>
</Window>
